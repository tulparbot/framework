{
    "name": "tulparbot/framework",
    "description": "The Tulpar Framework",
    "keywords": ["framework", "discord", "tulpar", "bot"],
    "license": "MIT",
    "homepage": "https://github.com/tulparbot",
    "support": {
        "issues": "https://github.com/tulparbot/tulpar/issues",
        "source": "https://github.com/tulparbot/tulpar"
    },
    "authors": [
        {
            "name": "Isa Eken",
            "email": "hello@isaeken.com.tr",
            "homepage": "https://www.isaeken.com.tr",
            "role": "Founder"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "replace": {
        "tulparbot/contracts": "self.version",
        "tulparbot/extension-container": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Tulparbot\\Framework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tulparbot\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
